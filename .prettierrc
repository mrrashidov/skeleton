{
  "singleQuote": true,
  "semi": false,
  "vueIndentScriptAndStyle": true,
  "printWidth": 100,
  "tabWidth": 4,
  "useTabs": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "trailingComma": "none",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "arrowParens": "always",
  "insertPragma": false,
  "requirePragma": false,
  "proseWrap": "never",
  "htmlWhitespaceSensitivity": "strict",
  "endOfLine": "lf"
}
